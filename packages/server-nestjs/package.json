{
  "name": "@torrin-kit/server-nestjs",
  "version": "0.8.0",
  "description": "NestJS integration for Torrin upload engine",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@torrin-kit/core": "workspace:*",
    "@torrin-kit/server": "workspace:*"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "reflect-metadata": "^0.1.13 || ^0.2.0",
    "rxjs": "^7.0.0"
  },
  "devDependencies": {
    "@nestjs/common": "^10.3.0",
    "@nestjs/core": "^10.3.0",
    "@types/express": "^4.17.21",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0"
  },
  "author": "Dimas Maulana",
  "repository": {
    "type": "git",
    "url": "https://github.com/dev-dimas/torrin.git"
  },
  "homepage": "https://github.com/dev-dimas/torrin/tree/main/packages/server-nestjs#readme",
  "keywords": [
    "torrin",
    "nestjs",
    "server",
    "upload",
    "file-upload",
    "resumable",
    "typescript"
  ],
  "sideEffects": false,
  "license": "Apache-2.0"
}
